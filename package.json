{
  "name": "nodan-scraper",
  "version": "0.2.3",
  "description": "Nodejs scraper for the modern web",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "prepare": "npm run build",
    "prepublishOnly": " npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kadajett/Nodan-Scraper.git"
  },
  "keywords": [
    "scraper",
    "typescript"
  ],
  "author": "Jeremy Stover (Kadajett)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kadajett/Nodan-Scraper/issues"
  },
  "homepage": "https://github.com/Kadajett/Nodan-Scraper#readme",
  "devDependencies": {
    "@types/bluebird": "^3.5.38",
    "@types/jest": "29.4.0",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.52.0",
    "eslint": "8.35.0",
    "eslint-plugin-jest": "27.2.1",
    "jest": "29.4.3",
    "prettier": "2.8.4",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "axios": "^1.4.0",
    "bluebird": "^3.7.2",
    "chalk": "^5.2.0",
    "cheerio": "^1.0.0-rc.12"
  }
}
